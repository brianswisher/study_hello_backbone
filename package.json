    "url-loader": "^0.5.5"
  },
  "devDependencies": {
    "browser-sync": "^1.9.0",
    "css-loader": "^0.8.0",
    "jsx-loader": "^0.12.2",
    "napa": "^1.2.0",
    "style-loader": "^0.8.0",
    "webpack": "^1.4.15"
  },
  "engines": {
    "node": "0.10.x"
